		"@sveltejs/adapter-static": "^3.0.10",
		"@sveltejs/kit": "^2.49.3",
		"@sveltejs/vite-plugin-svelte": "^6.2.2",
		"@tailwindcss/vite": "^4.1.18",
		"@types/jsonwebtoken": "^9.0.10",
		"bits-ui": "^2.15.4",
		"clsx": "^2.1.1",
		"eslint": "^9.39.2",
		"eslint-config-prettier": "^10.1.8",
		"eslint-plugin-svelte": "^3.13.1",
		"formsnap": "^2.0.1",
